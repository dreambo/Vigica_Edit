<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="vigica_edit.model.Service" table="SERVICE">
    <id column="IDX" name="s_idx" type="int"/>
    <property column="TYPE" name="s_type" type="string"/>
    <property column="NAME" name="s_name" type="string"/>
    <property column="NID" name="s_nid" type="int"/>
    <property column="PPR" name="s_ppr" type="string"/>
    <property column="LINE" name="s_line" type="text"/>
    <property column="FLAG" name="s_flag" type="boolean"/>
  </class>
</hibernate-mapping>
