<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="910.0" prefWidth="1130.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vigica.view.FXMLMainController">
    <children>
      <Button fx:id="openButton" onAction="#openAction" prefHeight="31.0" prefWidth="125.0" text="Open" textAlignment="CENTER" />
      <Button fx:id="saveButton" layoutX="140.0" onAction="#saveAction" prefHeight="31.0" prefWidth="125.0" text="Save" textAlignment="CENTER">
      	<tooltip><Tooltip text="Save to an other file" /></tooltip>
      </Button>
      <Button fx:id="compareButton" layoutX="280.0" onAction="#compareAction" prefHeight="31.0" prefWidth="125.0" text="Compare" textAlignment="CENTER">
      	<tooltip><Tooltip text="Compare with an other file" /></tooltip>
      </Button>
      <Button fx:id="duplicateButton" layoutX="414.0" onAction="#duplicateAction" prefHeight="31.0" prefWidth="125.0" text="Duplicate" textAlignment="CENTER">
      	<tooltip><Tooltip text="Remove duplicated entries" /></tooltip>
      </Button>
      <TextField fx:id="s_name" layoutX="193.0" layoutY="35.0" onKeyReleased="#filterAction" prefHeight="31.0" prefWidth="350.0" promptText="Name filter" />
      <ProgressBar fx:id="pi" layoutX="553.0" layoutY="6.0" prefHeight="23.0" prefWidth="550.0" progress="0.0" />
      <Label fx:id="title" layoutX="703.0" layoutY="39.0" prefHeight="23.0" prefWidth="375.0" textFill="#000000b6" />

      <TabPane layoutX="14.0" layoutY="73.0" prefHeight="601.0" prefWidth="636.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="DVB-S2">
               <content>
                  <TableView fx:id="serviceDVBS2Table" prefHeight="769.0" prefWidth="1024.0">
                     <columns>
                        <TableColumn fx:id="s_idxColumnS2" prefWidth="110.0" text="ID" />
                        <TableColumn fx:id="s_typeColumnS2" prefWidth="70.0" text="Type" />
                        <TableColumn fx:id="s_nidColumnS2" prefWidth="65.0" text="NID" />
                        <TableColumn fx:id="s_nameColumnS2" minWidth="0.0" prefWidth="480.0" text="Name" />
                        <TableColumn fx:id="s_pprColumnS2" maxWidth="334.0" minWidth="0.0" prefWidth="308.0" text="Preference" />
                        <TableColumn fx:id="s_newColumnS2" prefWidth="86.0" text="New" />
                     </columns>
                  </TableView>
               </content>
            </Tab>
          <Tab text="DVB-T2">
               <content>
                  <TableView fx:id="serviceDVBT2Table" prefHeight="769.0" prefWidth="1024.0">
                     <columns>
                        <TableColumn fx:id="s_idxColumnT2" prefWidth="110.0" text="ID" />
                        <TableColumn fx:id="s_typeColumnT2" prefWidth="70.0" text="Type" />
                        <TableColumn fx:id="s_nidColumnT2" prefWidth="65.0" text="NID" />
                        <TableColumn fx:id="s_nameColumnT2" minWidth="0.0" prefWidth="480.0" text="Name" />
                        <TableColumn fx:id="s_pprColumnT2" maxWidth="334.0" minWidth="0.0" prefWidth="308.0" text="Preference" />
                        <TableColumn fx:id="s_newColumnT2" prefWidth="86.0" text="New" />
                     </columns>
                  </TableView>
               </content>
            </Tab>
            <Tab text="PREFS">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
            <Tab text="CCCAM">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
    </children>
</AnchorPane>
